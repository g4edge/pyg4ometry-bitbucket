<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="100.0"/>
		<constant name="wby" value="100.0"/>
		<constant name="wbz" value="100.0"/>
		<constant name="bx" value="10.0"/>
		<constant name="by" value="20.0"/>
		<constant name="bz" value="30.0"/>
		<position name="center" x="0" y="0" z="0"/>
		<rotation name="identity" x="0" y="0" z="0"/>
		<position name="box1Vol_PV_position" x="0" y="0" z="0"/>
		<rotation name="box1Vol_PV_rotation" x="0" y="0" z="0"/>
	</define>
	<materials>
    <!--Isotopes-->
    <isotope name="U235" Z="92" N="235">
      <atom type="A" value="235.01"/>

    </isotope>
    <isotope name="U238" Z="92" N="238">
      <atom type="A" value="235.03"/>
    </isotope>
    <!--Elements-->
     <element Z="8" formula="O" name="Oxygen" >
       <atom value="16" />
     </element>
     <element Z="1" formula="H" name="Hydrogen" >
       <atom value="1" />
     </element>
     <element name="Nitrogen" formula="N"  Z="7.">
       <atom value="14.01"/>
     </element>

     <element name="enriched_uranium" >
       <fraction ref="U235" n="0.9" />
       <fraction ref="U238" n="0.1" />
     </element>
     <!--Materials-->
      <material name="Al" Z="13.0" >
        <D value="2.70" />
        <atom value="26.98" />
      </material>

      <material name="Water" formula="H2O">
        <D value="1.0" />
        <composite n="2" ref="Hydrogen" />
        <composite n="1" ref="Oxygen" />
      </material>

      <material formula="air" name="Air" >
        <D value="0.001289" />
        <fraction n="0.7" ref="Nitrogen" />
        <fraction n="0.3" ref="Oxygen" />
      </material>
	</materials>

	<solids>
		<box name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<box name="box1" x="bx" y="by" z="bz"/>
	</solids>
	<structure>
		<volume name="box1Vol">
			<materialref ref="G4_Galactic"/>
			<solidref ref="box1"/>
		</volume>
		<volume name="World">
			<materialref ref="G4_Galactic"/>
			<solidref ref="WorldBox"/>
			<physvol name="box1Vol_PV">
				<volumeref ref="box1Vol"/>
				<positionref ref="box1Vol_PV_position"/>
				<rotationref ref="box1Vol_PV_rotation"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

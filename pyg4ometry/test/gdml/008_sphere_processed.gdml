<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="100.0"/>
		<constant name="wby" value="100.0"/>
		<constant name="wbz" value="100.0"/>
		<constant name="srmin" value="40.0"/>
		<constant name="srmax" value="45.0"/>
		<constant name="sdphi" value="pi"/>
		<constant name="sdtheta" value="3*pi/4"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
	</define>
	<materials>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="m" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<sphere aunit="rad" deltaphi="sdphi" deltatheta="sdtheta" lunit="m" name="sphere1" rmax="srmax" rmin="srmin" startphi="0" starttheta="0"/>
	</solids>
	<structure>
		<volume name="sphere1Vol">
			<materialref ref="Air"/>
			<solidref ref="sphere1"/>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="sphere1Vol_PV">
				<volumeref ref="sphere1Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<userinfo/>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="100.0"/>
		<constant name="wby" value="100.0"/>
		<constant name="wbz" value="100.0"/>
		<position name="v1" unit="mm" x="10" y="10" z="0"/>
		<position name="v2" unit="mm" x="-10" y="10" z="0"/>
		<position name="v3" unit="mm" x="-10" y="-10" z="0"/>
		<position name="v4" unit="mm" x="0" y="0" z="10"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
		<position name="tet1_v1" x="10" y="10" z="0"/>
		<position name="tet1_v2" x="-10" y="10" z="0"/>
		<position name="tet1_v3" x="-10" y="-10" z="0"/>
		<position name="tet1_v4" x="0" y="0" z="10"/>
	</define>
	<materials>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<tet lunit="mm" name="tet1" vertex1="tet1_v1" vertex2="tet1_v2" vertex3="tet1_v3" vertex4="tet1_v4"/>
	</solids>
	<structure>
		<volume name="tet1Vol">
			<materialref ref="Air"/>
			<solidref ref="tet1"/>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="tet1Vol_PV">
				<volumeref ref="tet1Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<userinfo/>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="1000.0"/>
		<constant name="wby" value="1000.0"/>
		<constant name="wbz" value="1000.0"/>
		<constant name="bx" value="5.0"/>
		<constant name="by" value="bx"/>
		<constant name="bz" value="bx"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
	</define>
	<materials>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<box lunit="mm" name="box" x="20*bx" y="20*bx" z="20*bx"/>
		<polycone aunit="rad" deltaphi="4" lunit="mm" name="polycone_param" startphi="1">
			<zplane rmax="9" rmin="1" z="-5"/>
			<zplane rmax="5" rmin="3" z="0"/>
			<zplane rmax="9" rmin="1" z="5"/>
		</polycone>
	</solids>
	<structure>
		<volume name="polycone1Vol">
			<materialref ref="Al"/>
			<solidref ref="polycone_param"/>
		</volume>
		<volume name="polycone2Vol">
			<materialref ref="Air"/>
			<solidref ref="box"/>
			<paramvol ncopies="9">
				<volumeref ref="polycone1Vol"/>
				<parameterised_position_size>
					<parameters number="1">
						<position name="polycone1Vol1pos" unit="mm" x="0" y="0" z="-8*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.9" startPhi="2*pi*0.1">
							<zplane rmax="9" rmin="1" z="-0.5"/>
							<zplane rmax="5" rmin="3" z="0"/>
							<zplane rmax="9" rmin="1" z="0.5"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="2">
						<position name="polycone1Vol2pos" unit="mm" x="0" y="0" z="-6*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.8" startPhi="2*pi*0.2">
							<zplane rmax="10" rmin="2" z="-1.0"/>
							<zplane rmax="6" rmin="3" z="0"/>
							<zplane rmax="10" rmin="2" z="1.0"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="3">
						<position name="polycone1Vol3pos" unit="mm" x="0" y="0" z="-4*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.7" startPhi="2*pi*0.3">
							<zplane rmax="11" rmin="3" z="-1.0"/>
							<zplane rmax="7" rmin="4" z="0"/>
							<zplane rmax="11" rmin="3" z="1.0"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="4">
						<position name="polycone1Vol3pos" unit="mm" x="0" y="0" z="-2*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.6" startPhi="2*pi*0.4">
							<zplane rmax="12" rmin="4" z="-1.5"/>
							<zplane rmax="8" rmin="5" z="0"/>
							<zplane rmax="12" rmin="4" z="1.5"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="5">
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.5" startPhi="2*pi*0.5">
							<zplane rmax="13" rmin="5" z="-2"/>
							<zplane rmax="9" rmin="6" z="0"/>
							<zplane rmax="13" rmin="5" z="2"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="6">
						<position name="polycone1Vol6pos" unit="mm" x="0" y="0" z="2*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.4" startPhi="2*pi*0.6">
							<zplane rmax="14" rmin="6" z="-2.5"/>
							<zplane rmax="10" rmin="7" z="0"/>
							<zplane rmax="14" rmin="6" z="2.5"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="7">
						<position name="polycone1Vol7pos" unit="mm" x="0" y="0" z="4*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.3" startPhi="2*pi*0.7">
							<zplane rmax="15" rmin="7" z="-3"/>
							<zplane rmax="11" rmin="8" z="0"/>
							<zplane rmax="15" rmin="7" z="3"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="8">
						<position name="polycone1Vol8pos" unit="mm" x="0" y="0" z="6*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.2" startPhi="2*pi*0.8">
							<zplane rmax="16" rmin="8" z="-3.5"/>
							<zplane rmax="12" rmin="9" z="0"/>
							<zplane rmax="16" rmin="8" z="3.5"/>
						</polycone_dimensions>
					</parameters>
					<parameters number="9">
						<position name="polycone1Vol9pos" unit="mm" x="0" y="0" z="8*bx"/>
						<polycone_dimensions aunit="rad" lunit="mm" numRZ="3" openPhi="2*pi*0.1" startPhi="2*pi*0.9">
							<zplane rmax="17" rmin="9" z="-4.0"/>
							<zplane rmax="13" rmin="10" z="0"/>
							<zplane rmax="17" rmin="9" z="4.0"/>
						</polycone_dimensions>
					</parameters>
				</parameterised_position_size>
			</paramvol>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="polycone2Vol_PV">
				<volumeref ref="polycone2Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

<?xml version="1.0" encoding="UTF-8" ?>
<gdml xmlns:gdml="http://cern.ch/2001/Schemas/GDML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd" >

<define>
  <constant name="wbx" value="200.0"/>
  <constant name="wby" value="200.0"/>
  <constant name="wbz" value="200.0"/>

  <constant name="bx" value="10.0"/>
  
  <position name="center"    x="0"   y="0"   z="0"/> 
  <rotation name="identity"  x="0"   y="0"   z="0"/>
</define>

<materials>
  <element name="Oxygen"   formula="O"  Z="8.">
    <atom value="16.0"/>
  </element>
  <element name="Nitrogen" formula="N"  Z="7.">
    <atom value="14.01"/>
  </element>

  <material name="Al" Z="13.0">
   <D value="2.70"/>
   <atom value="26.98"/>
  </material>

  <material name="Air">
   <D value="1.290" unit="mg/cm3"/>
   <fraction n="0.7" ref="Nitrogen"/>
   <fraction n="0.3" ref="Oxygen"/>
  </material>

</materials>

<solids>
  <box name="WorldBox" x="wbx"  y="wby"  z="wbz" />
  <box name="box"      x="1.0*bx"   y="1.0*bx"   z="0.5*bx"  />
  <box name="row"      x="bx" y="bx"   z="bx" />
</solids>

<structure>

  <volume name="boxVol"> 
    <materialref ref="Air"/>
    <solidref ref="box"/>
  </volume>

  <volume name="rowVol"> 
    <materialref ref="Air"/>
    <solidref ref="row"/>

    <physvol>
       <volumeref ref="boxVol"/>
       <position name="boxVol_pos" x="0" y="0" z="-0.5*bx/2.0"/>
       <rotationref ref="identity"/>
    </physvol>

  </volume>

  <volume name="World">
    <materialref ref="Air"/>
    <solidref ref="WorldBox"/>

    <physvol>       
       <volumeref ref="rowVol"/>
       <position name="rowVol_pos" x="0" y="0" z="0"/>
       <rotationref ref="identity"/>
    </physvol>      
  </volume>
</structure>

<setup name="Default" version="1.0">
  <world ref="World"/>
</setup>

</gdml>

<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<position name="union_position2" unit="mm" x="10" y="0" z="75.0"/>
		<rotation name="union_rotation2" unit="deg" x="0.0" y="0.0" z="30"/>
	</define>
	<materials>
		<material Z="13" name="ALU" state="solid">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material Z="1" name="AIR" state="gas">
			<D value="1e-24"/>
			<atom value="1.00794"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="1000" y="1000" z="1000"/>
		<box lunit="mm" name="box_first" x="1" y="5" z="20"/>
		<box lunit="mm" name="box_second" x="4" y="4.5" z="18"/>
		<multiUnion name="multiunion1">
			<multiUnionNode name="multiunion1-node-1">
				<solid ref="box_first"/>
			</multiUnionNode>
			<multiUnionNode name="multiunion1-node-2">
				<solid ref="box_second"/>
				<positionref ref="union_position2"/>
				<rotationref ref="union_rotation2"/>
			</multiUnionNode>
		</multiUnion>
	</solids>
	<structure>
		<volume name="lvmultiunion">
			<materialref ref="ALU"/>
			<solidref ref="multiunion1"/>
		</volume>
		<volume name="TOP">
			<materialref ref="AIR"/>
			<solidref ref="WorldBox"/>
			<physvol name="lvCube_phys">
				<volumeref ref="lvmultiunion"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="TOP"/>
	</setup>
</gdml>

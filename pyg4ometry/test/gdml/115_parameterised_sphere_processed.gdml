<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="1000.0"/>
		<constant name="wby" value="1000.0"/>
		<constant name="wbz" value="1000.0"/>
		<constant name="bx" value="5.0"/>
		<constant name="by" value="bx"/>
		<constant name="bz" value="bx"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
	</define>
	<materials>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<box lunit="mm" name="box" x="20*bx" y="20*bx" z="20*bx"/>
		<sphere aunit="rad" deltaphi="2*pi" deltatheta="0.1*pi" lunit="mm" name="sphere_param" rmax="1.2" rmin="1.0" startphi="0" starttheta="0"/>
	</solids>
	<structure>
		<volume name="sphere1Vol">
			<materialref ref="Al"/>
			<solidref ref="sphere_param"/>
		</volume>
		<volume name="box2Vol">
			<materialref ref="Air"/>
			<solidref ref="box"/>
			<paramvol ncopies="9">
				<volumeref ref="sphere1Vol"/>
				<parameterised_position_size>
					<parameters number="1">
						<position name="sphere1Vol1pos" unit="mm" x="0" y="0" z="-8*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.1" deltatheta="pi*0.1" lunit="mm" rmax="0.4" rmin="0.2" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="2">
						<position name="sphere1Vol2pos" unit="mm" x="0" y="0" z="-6*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.2" deltatheta="pi*0.2" lunit="mm" rmax="0.6" rmin="0.3" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="3">
						<position name="sphere1Vol3pos" unit="mm" x="0" y="0" z="-4*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.3" deltatheta="pi*0.3" lunit="mm" rmax="0.8" rmin="0.4" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="4">
						<position name="sphere1Vol4pos" unit="mm" x="0" y="0" z="-2*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.4" deltatheta="pi*0.4" lunit="mm" rmax="1.0" rmin="0.5" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="5">
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.5" deltatheta="pi*0.5" lunit="mm" rmax="1.2" rmin="0.6" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="6">
						<position name="sphere1Vol6pos" unit="mm" x="0" y="0" z="2*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.6" deltatheta="pi*0.6" lunit="mm" rmax="1.4" rmin="0.7" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="7">
						<position name="sphere1Vol7pos" unit="mm" x="0" y="0" z="4*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.7" deltatheta="pi*0.7" lunit="mm" rmax="1.6" rmin="0.8" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="8">
						<position name="sphere1Vol8pos" unit="mm" x="0" y="0" z="6*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.8" deltatheta="pi*0.8" lunit="mm" rmax="1.8" rmin="0.9" startphi="0" starttheta="0"/>
					</parameters>
					<parameters number="9">
						<position name="sphere1Vol9pos" unit="mm" x="0" y="0" z="8*bx"/>
						<sphere_dimensions aunit="rad" deltaphi="2*pi*0.9" deltatheta="pi*0.9" lunit="mm" rmax="2.0" rmin="1.0" startphi="0" starttheta="0"/>
					</parameters>
				</parameterised_position_size>
			</paramvol>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="box2Vol_PV">
				<volumeref ref="box2Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

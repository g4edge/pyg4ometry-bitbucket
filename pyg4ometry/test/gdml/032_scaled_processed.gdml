<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="200.0"/>
		<constant name="wby" value="200.0"/>
		<constant name="wbz" value="200.0"/>
		<constant name="bx" value="10.0"/>
		<constant name="by" value="10.0"/>
		<constant name="bz" value="10.0"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
	</define>
	<materials>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<box lunit="mm" name="box1" x="bx" y="by" z="bz"/>
		<scaledSolid name="box1Scaled">
			<solidref ref="box1"/>
			<scale x="1" y="2" z="3"/>
		</scaledSolid>
	</solids>
	<structure>
		<volume name="box1Vol">
			<materialref ref="Air"/>
			<solidref ref="box1"/>
		</volume>
		<volume name="box2Vol">
			<materialref ref="Air"/>
			<solidref ref="box1Scaled"/>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="box1Vol_PV">
				<volumeref ref="box1Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
			<physvol name="box2Vol_PV">
				<volumeref ref="box2Vol"/>
				<position unit="mm" x="0" y="0" z="50"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<userinfo/>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="100.0"/>
		<constant name="wby" value="100.0"/>
		<constant name="wbz" value="100.0"/>
		<constant name="xv1x" value="3.0"/>
		<constant name="xv1y" value="9.0"/>
		<constant name="xv2x" value="1.0"/>
		<constant name="xv2y" value="5.0"/>
		<constant name="xv3x" value="2.0"/>
		<constant name="xv3y" value="4.0"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
	</define>
	<materials>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<xtru lunit="mm" name="xtru1">
			<twoDimVertex x="xv1x" y="xv1y"/>
			<twoDimVertex x="xv2x" y="xv2y"/>
			<twoDimVertex x="xv3x" y="xv3y"/>
			<section scalingFactor="3" xOffset="5" yOffset="3" zOrder="0" zPosition="2"/>
			<section scalingFactor="1" xOffset="3" yOffset="5" zOrder="1" zPosition="5"/>
		</xtru>
	</solids>
	<structure>
		<volume name="xtru1Vol">
			<materialref ref="Air"/>
			<solidref ref="xtru1"/>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="xtru1Vol_PV">
				<volumeref ref="xtru1Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

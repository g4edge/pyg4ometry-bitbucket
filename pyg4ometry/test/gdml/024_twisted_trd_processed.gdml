<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="100.0"/>
		<constant name="wby" value="100.0"/>
		<constant name="wbz" value="100.0"/>
		<constant name="ttdtwist" value="1.0"/>
		<constant name="ttdx1" value="35.0"/>
		<constant name="ttdx2" value="32.0"/>
		<constant name="ttdy1" value="24.0"/>
		<constant name="ttdy2" value="20.0"/>
		<constant name="ttdz" value="60.0"/>
		<position name="center" unit="mm" x="0" y="0" z="0"/>
		<rotation name="identity" unit="rad" x="0" y="0" z="0"/>
	</define>
	<materials>
		<element Z="8" formula="O" name="Oxygen">
			<atom value="16.0"/>
		</element>
		<element Z="7" formula="N" name="Nitrogen">
			<atom value="14.01"/>
		</element>
		<material Z="13" name="Al">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction n="0.7" ref="Nitrogen"/>
			<fraction n="0.3" ref="Oxygen"/>
		</material>
	</materials>
	<solids>
		<box lunit="mm" name="WorldBox" x="wbx" y="wby" z="wbz"/>
		<twistedtrd PhiTwist="ttdtwist" aunit="rad" lunit="mm" name="twisttrd1" x1="2*ttdx1" x2="2*ttdx2" y1="2*ttdy1" y2="2*ttdy2" z="2*ttdz"/>
	</solids>
	<structure>
		<volume name="twisttrd1Vol">
			<materialref ref="Air"/>
			<solidref ref="twisttrd1"/>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="twisttrd1Vol_PV">
				<volumeref ref="twisttrd1Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="100"/>
		<constant name="wy" value="100"/>
		<constant name="wz" value="100"/>
		<constant name="trmin" value="2.5"/>
		<constant name="trmax" value="10.0"/>
		<constant name="tz" value="50"/>
		<constant name="startphi" value="0"/>
		<constant name="deltaphi" value="1.5*pi"/>
	</define>
	<materials/>
	<solids>
		<box lunit="mm" name="ws" x="wx" y="wy" z="wz"/>
		<tube aunit="rad" deltaphi="deltaphi" lunit="mm" name="ts" rmax="trmax" rmin="trmin" startphi="startphi" z="tz"/>
	</solids>
	<structure>
		<volume name="tl">
			<materialref ref="G4_Fe"/>
			<solidref ref="ts"/>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="t_pv1">
				<volumeref ref="tl"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>

<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wx" value="100"/>
		<constant name="wy" value="100"/>
		<constant name="wz" value="100"/>
		<constant name="px" value="10"/>
		<constant name="py" value="20"/>
		<constant name="pz" value="30"/>
		<constant name="pAlpha" value="0.2"/>
		<constant name="pTheta" value="0.3"/>
		<constant name="pPhi" value="0.4"/>
	</define>
	<materials/>
	<solids>
		<box lunit="mm" name="ws" x="wx" y="wy" z="wz"/>
		<para alpha="pAlpha" aunit="rad" lunit="mm" name="ps" phi="pPhi" theta="pTheta" x="px" y="py" z="pz"/>
	</solids>
	<structure>
		<volume name="pl">
			<materialref ref="G4_Fe"/>
			<solidref ref="ps"/>
		</volume>
		<volume name="wl">
			<materialref ref="G4_Galactic"/>
			<solidref ref="ws"/>
			<physvol name="p_pv1">
				<volumeref ref="pl"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="wl"/>
	</setup>
</gdml>

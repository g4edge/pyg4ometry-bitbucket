beam,  particle="e-",
       energy=1.0 * GeV,
       distrType="gauss",
       X0=0*mm,
       Y0=0*mm,
       sigmaX=0.002*mm,
       sigmaY=0.002*mm,
       sigmaXp=0.06,
       sigmaYp=0.06;

option, beampipeRadius = 1 * cm,
        boxSize = 1.9 * m,
        tunnelRadius= 2.0 * m,
        beampipeThickness = 0.2 * cm,
        chordStepMinimum = 0.0000000001 * m,
        deltaIntersection = 0.00000001 * m,
!        deltaChord = 0.001 * m,
        lengthSafety = 0.00001 * m,
        thresholdCutCharged = 100 * MeV,
        thresholdCutPhotons = 1000 * KeV,
        storeTrajectory = 1,    
        checkOverlaps = 1;


option,physicsList="em";
option,ngenerate=5000;

d01 : drift, l=0.1*m;
e01 : element, geometry="gdml:g4_examples_extended_persistency_gdml_g01.gdml", outerDiameter=10000*mm, l=10000*mm;
! e01 : quadrupole, k1=10, l=88.5*mm;
d02 : drift, l=0.1*m;

lat : line = (d01,e01,d02);
use, period=lat;

sample, range = d01;
sample, range = e01;
sample, range = d02;
        deltaIntersection = 0.00000001 * m,
!        deltaChord = 0.001 * m,
        lengthSafety = 0.00001 * m,
        thresholdCutCharged = 100 * MeV,
        thresholdCutPhotons = 1000 * KeV,
        storeTrajectory = 1,    

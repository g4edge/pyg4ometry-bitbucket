beam,  particle="e-",
       energy=1.0 * GeV,
       distrType="gauss",
       X0=0*mm,
       Y0=0*mm,
       sigmaX=0.002*mm,
       sigmaY=0.002*mm,
       sigmaXp=0.06,
       sigmaYp=0.06;

option, beampipeRadius = 1 * cm,
        boxSize = 1.9 * m,
        tunnelRadius= 2.0 * m,
        beampipeThickness = 0.2 * cm,

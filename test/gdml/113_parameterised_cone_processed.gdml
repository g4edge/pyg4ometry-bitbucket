<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="1000.0"/>
		<constant name="wby" value="1000.0"/>
		<constant name="wbz" value="1000.0"/>
		<constant name="bx" value="5.0"/>
		<constant name="by" value="bx"/>
		<constant name="bz" value="bx"/>
		<position name="center" x="0" y="0" z="0" unit="mm"/>
		<rotation name="identity" x="0" y="0" z="0" unit="rad"/>
	</define>
	<materials>
		<element name="Oxygen" formula="O" Z="8">
			<atom value="16.0"/>
		</element>
		<element name="Nitrogen" formula="N" Z="7">
			<atom value="14.01"/>
		</element>
		<material name="Al" Z="13">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction ref="Nitrogen" n="0.7"/>
			<fraction ref="Oxygen" n="0.3"/>
		</material>
	</materials>
	<solids>
		<box name="WorldBox" x="wbx" y="wby" z="wbz" lunit="mm"/>
		<box name="box" x="20*bx" y="20*bx" z="20*bx" lunit="mm"/>
		<cone name="cone_param" rmin1="0.1" rmax1="0.2" rmin2="0.5" rmax2="1.0" z="1" startphi="0" deltaphi="2*pi" lunit="mm" aunit="rad"/>
	</solids>
	<structure>
		<volume name="cone1Vol">
			<materialref ref="Al"/>
			<solidref ref="cone_param"/>
		</volume>
		<volume name="box2Vol">
			<materialref ref="Air"/>
			<solidref ref="box"/>
			<paramvol ncopies="9">
				<volumeref ref="cone1Vol"/>
				<parameterised_position_size>
					<parameters number="1">
						<position name="cone1Vol1pos" x="0" y="0" z="-8*bx" unit="mm"/>
						<rotation name="cone11Vol1Rot" x="0" y="0" z="-0.4" unit="rad"/>
						<cone_dimensions rmin1="0.1" rmax1="0.2" rmin2="0.5" rmax2="1.0" z="2.5" startphi="0" deltaphi="2*pi*0.1" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="2">
						<position name="cone1Vol2pos" x="0" y="0" z="-6*bx" unit="mm"/>
						<rotation name="cone1Vol2Rot" x="0" y="0" z="-0.3" unit="rad"/>
						<cone_dimensions rmin1="0.2" rmax1="0.4" rmin2="0.6" rmax2="1.4" z="2.5" startphi="0" deltaphi="2*pi*0.2" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="3">
						<position name="cone1Vol3pos" x="0" y="0" z="-4*bx" unit="mm"/>
						<rotation name="cone1Vol3Rot" x="0" y="0" z="-0.2" unit="rad"/>
						<cone_dimensions rmin1="0.3" rmax1="0.6" rmin2="0.7" rmax2="1.8" z="2.5" startphi="0" deltaphi="2*pi*0.3" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="4">
						<position name="cone1Vol4pos" x="0" y="0" z="-2*bx" unit="mm"/>
						<rotation name="cone1Vol24ot" x="0" y="0" z="-0.1" unit="rad"/>
						<cone_dimensions rmin1="0.4" rmax1="0.8" rmin2="0.8" rmax2="2.2" z="2.5" startphi="0" deltaphi="2*pi*0.4" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="5">
						<cone_dimensions rmin1="0.5" rmax1="1.0" rmin2="0.9" rmax2="2.6" z="2.5" startphi="0" deltaphi="2*pi*0.5" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="6">
						<position name="cone1Vol6pos" x="0" y="0" z="2*bx" unit="mm"/>
						<rotation name="cone1Vol6Rot" x="0" y="0" z="0.1" unit="rad"/>
						<cone_dimensions rmin1="0.6" rmax1="1.2" rmin2="1.0" rmax2="3.0" z="2.5" startphi="0" deltaphi="2*pi*0.6" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="7">
						<position name="cone1Vol7pos" x="0" y="0" z="4*bx" unit="mm"/>
						<rotation name="cone1Vol7Rot" x="0" y="0" z="0.2" unit="rad"/>
						<cone_dimensions rmin1="0.7" rmax1="1.4" rmin2="1.1" rmax2="3.4" z="2.5" startphi="0" deltaphi="2*pi*0.7" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="8">
						<position name="cone1Vol8pos" x="0" y="0" z="6*bx" unit="mm"/>
						<rotation name="cone1Vol8Rot" x="0" y="0" z="0.3" unit="rad"/>
						<cone_dimensions rmin1="0.8" rmax1="1.6" rmin2="1.2" rmax2="3.8" z="2.5" startphi="0" deltaphi="2*pi*0.8" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="9">
						<position name="cone1Vol9pos" x="0" y="0" z="8*bx" unit="mm"/>
						<rotation name="cone1Vol9Rot" x="0" y="0" z="0.4" unit="rad"/>
						<cone_dimensions rmin1="0.9" rmax1="1.8" rmin2="1.3" rmax2="4.2" z="2.5" startphi="0" deltaphi="2*pi*0.9" lunit="mm" aunit="rad"/>
					</parameters>
				</parameterised_position_size>
			</paramvol>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="box2Vol_PV">
				<volumeref ref="box2Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

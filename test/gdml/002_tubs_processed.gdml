<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="100.0"/>
		<constant name="wby" value="100.0"/>
		<constant name="wbz" value="100.0"/>
		<constant name="rmin" value="2.5"/>
		<constant name="rmax" value="10.0"/>
		<constant name="z" value="50"/>
		<constant name="deltaphi" value="1.5*pi"/>
		<position name="center" x="0" y="0" z="0" unit="mm"/>
		<rotation name="identity" x="0" y="0" z="0" unit="rad"/>
	</define>
	<materials>
		<element name="Oxygen" formula="O" Z="8">
			<atom value="16.0"/>
		</element>
		<element name="Nitrogen" formula="N" Z="7">
			<atom value="14.01"/>
		</element>
		<material name="Al" Z="13">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction ref="Nitrogen" n="0.7"/>
			<fraction ref="Oxygen" n="0.3"/>
		</material>
	</materials>
	<solids>
		<box name="WorldBox" x="wbx" y="wby" z="wbz" lunit="mm"/>
		<tube name="tube1" rmin="rmin" rmax="rmax" z="z" startphi="0" deltaphi="deltaphi" lunit="mm" aunit="rad"/>
	</solids>
	<structure>
		<volume name="tube1Vol">
			<materialref ref="Air"/>
			<solidref ref="tube1"/>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="tube1Vol_PV">
				<volumeref ref="tube1Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>

<?xml version="1.0" encoding="UTF-8" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">

   <define>
   </define>

   <materials>
      <material Z="1.0" name="AIR" state="gas">
         <D value="1e-24"/>
         <atom value="1.00794"/>
      </material>
      <material name="ALU" state="solid" Z="13.0">
         <D value="2.70"/>
         <atom value="26.98"/>
      </material>
   </materials>

   <solids>
      <box name="WorldBox" x="1000" y="1000" z="1000"/>
      <polycone name = "Polyc" startphi="1" deltaphi="4" aunit="rad" lunit="mm">
          <zplane rmin="1" rmax="9" z="-20" />
          <zplane rmin="3" rmax="5" z="0" />
          <zplane rmin="1" rmax="9" z="20" />
      </polycone>
   </solids>
   
   <structure>
      <volume name="lvPolyc">
         <materialref ref="ALU"/>
         <solidref ref="Polyc"/>
      </volume>
      <volume name="lvDivision">
         <materialref ref="AIR"/>
         <solidref ref="Polyc"/>
         <divisionvol axis="kZAxis" number="4" unit="mm" width="4" offset="3">
            <volumeref ref="lvPolyc"/>
         </divisionvol>
      </volume>
      <volume name="TOP">
         <materialref ref="AIR"/>
         <solidref ref="WorldBox"/>
         <physvol>
            <volumeref ref="lvDivision"/>
         </physvol>
      </volume>
   </structure>

   <setup version="1.0" name="Default">
      <world ref="TOP"/>
   </setup>

</gdml>

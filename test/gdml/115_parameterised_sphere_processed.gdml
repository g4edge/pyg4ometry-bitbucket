<?xml version="1.0" ?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">
	<define>
		<constant name="wbx" value="1000.0"/>
		<constant name="wby" value="1000.0"/>
		<constant name="wbz" value="1000.0"/>
		<constant name="bx" value="5.0"/>
		<constant name="by" value="bx"/>
		<constant name="bz" value="bx"/>
		<position name="center" x="0" y="0" z="0" unit="mm"/>
		<rotation name="identity" x="0" y="0" z="0" unit="rad"/>
	</define>
	<materials>
		<element name="Oxygen" formula="O" Z="8">
			<atom value="16.0"/>
		</element>
		<element name="Nitrogen" formula="N" Z="7">
			<atom value="14.01"/>
		</element>
		<material name="Al" Z="13">
			<D value="2.7"/>
			<atom value="26.98"/>
		</material>
		<material name="Air">
			<D value="1.29"/>
			<fraction ref="Nitrogen" n="0.7"/>
			<fraction ref="Oxygen" n="0.3"/>
		</material>
	</materials>
	<solids>
		<box name="WorldBox" x="wbx" y="wby" z="wbz" lunit="mm"/>
		<box name="box" x="20*bx" y="20*bx" z="20*bx" lunit="mm"/>
		<sphere name="sphere_param" rmin="1.0" rmax="1.2" deltaphi="2*pi" startphi="0" starttheta="0" deltatheta="0.1*pi" lunit="mm" aunit="rad"/>
	</solids>
	<structure>
		<volume name="sphere1Vol">
			<materialref ref="Al"/>
			<solidref ref="sphere_param"/>
		</volume>
		<volume name="box2Vol">
			<materialref ref="Air"/>
			<solidref ref="box"/>
			<paramvol ncopies="9">
				<volumeref ref="sphere1Vol"/>
				<parameterised_position_size>
					<parameters number="1">
						<position name="sphere1Vol1pos" x="0" y="0" z="-8*bx" unit="mm"/>
						<sphere_dimensions rmin="0.2" rmax="0.4" startphi="0" deltaphi="2*pi*0.1" starttheta="0" deltatheta="pi*0.1" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="2">
						<position name="sphere1Vol2pos" x="0" y="0" z="-6*bx" unit="mm"/>
						<sphere_dimensions rmin="0.3" rmax="0.6" startphi="0" deltaphi="2*pi*0.2" starttheta="0" deltatheta="pi*0.2" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="3">
						<position name="sphere1Vol3pos" x="0" y="0" z="-4*bx" unit="mm"/>
						<sphere_dimensions rmin="0.4" rmax="0.8" startphi="0" deltaphi="2*pi*0.3" starttheta="0" deltatheta="pi*0.3" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="4">
						<position name="sphere1Vol4pos" x="0" y="0" z="-2*bx" unit="mm"/>
						<sphere_dimensions rmin="0.5" rmax="1.0" startphi="0" deltaphi="2*pi*0.4" starttheta="0" deltatheta="pi*0.4" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="5">
						<sphere_dimensions rmin="0.6" rmax="1.2" startphi="0" deltaphi="2*pi*0.5" starttheta="0" deltatheta="pi*0.5" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="6">
						<position name="sphere1Vol6pos" x="0" y="0" z="2*bx" unit="mm"/>
						<sphere_dimensions rmin="0.7" rmax="1.4" startphi="0" deltaphi="2*pi*0.6" starttheta="0" deltatheta="pi*0.6" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="7">
						<position name="sphere1Vol7pos" x="0" y="0" z="4*bx" unit="mm"/>
						<sphere_dimensions rmin="0.8" rmax="1.6" startphi="0" deltaphi="2*pi*0.7" starttheta="0" deltatheta="pi*0.7" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="8">
						<position name="sphere1Vol8pos" x="0" y="0" z="6*bx" unit="mm"/>
						<sphere_dimensions rmin="0.9" rmax="1.8" startphi="0" deltaphi="2*pi*0.8" starttheta="0" deltatheta="pi*0.8" lunit="mm" aunit="rad"/>
					</parameters>
					<parameters number="9">
						<position name="sphere1Vol9pos" x="0" y="0" z="8*bx" unit="mm"/>
						<sphere_dimensions rmin="1.0" rmax="2.0" startphi="0" deltaphi="2*pi*0.9" starttheta="0" deltatheta="pi*0.9" lunit="mm" aunit="rad"/>
					</parameters>
				</parameterised_position_size>
			</paramvol>
		</volume>
		<volume name="World">
			<materialref ref="Air"/>
			<solidref ref="WorldBox"/>
			<physvol name="box2Vol_PV">
				<volumeref ref="box2Vol"/>
				<positionref ref="center"/>
				<rotationref ref="identity"/>
			</physvol>
		</volume>
	</structure>
	<setup name="Default" version="1.0">
		<world ref="World"/>
	</setup>
</gdml>
